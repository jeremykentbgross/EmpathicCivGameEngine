<!--!!
	* document: file
	* description: This file is a template for the ECGE Documentation Generation.  (Creates Method Index)
-->

<h3>Methods:</h3>
<table>
	<tbody>
		<tr>
			<td class="tableColumnLabel">ReturnType(s):</td>
			<td class="tableColumnLabel">Origin:</td>
			<td class="tableColumnLabel">Name:</td>
			<!--<td class="tableColumnLabel">Description:</td>-->
		</tr>
		{{#each	containedMethods}}
		<tr style="{{isOverrideStyle ../this this}}">
			<td>{{> types returns}}</td>
			<td>
				{{#if namespace}}
					<a href="/_protected_/docs/generated/namespaces/{{namespace}}.html" target="mainFrame">{{namespace}}</a>
				{{/if}}
				{{#if class}}
					<a href="/_protected_/docs/generated/classes/{{class}}.html" target="mainFrame">{{class}}</a>
				{{/if}}
			</td>
			<td><a href="#{{#if namespace}}{{namespace}}.{{/if}}{{#if class}}{{class}}.{{/if}}{{name}}">{{name}}</a>({{#each parameters}}{{#if @index}}, {{/if}}{{name}}{{/each}})
				<!--<br/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;* {{description}}-->
			</td>
			<!--<td>{{description}}</td>-->
		</tr></s>
		{{/each}}
	</tbody>
</table>

