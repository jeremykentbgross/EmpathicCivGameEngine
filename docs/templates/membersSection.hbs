<!--!!
	* document: file
	* description: This file is a template for the ECGE Documentation Generation.  (Creates Member Variable Section)
-->

<h3>Members:</h3>
{{#each	containedMembers}}
	<h4 style="{{isOverrideStyle ../this this}}" id="{{#if namespace}}{{namespace}}.{{/if}}{{#if class}}{{class}}.{{/if}}{{name}}">Member: {{name}}</h4>
	<table style="{{isOverrideStyle ../this this}}">
		<tbody>
			{{> basicProperties this}}
			<tr><td class="tableRowLabel">Type(s):</td><td>{{> types types}}</td></tr>
			<tr><td class="tableRowLabel">Default:</td><td>{{default}}</td></tr>
		</tbody>
	</table>
{{/each}}


